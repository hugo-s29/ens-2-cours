
<?xml version='1.0'?>
<!DOCTYPE platform SYSTEM "https://simgrid.org/simgrid.dtd">
<platform version="4.1">

<config>
	<prop id="smpi/host-speed" value="100Gf"></prop>
</config>

<zone id="AS0" routing="Dijkstra">
	<host id="host-0.simgrid.org" speed="1000Gf"/>
	<host id="host-1.simgrid.org" speed="1000Gf"/>
	<host id="host-2.simgrid.org" speed="1000Gf"/>
	<host id="host-3.simgrid.org" speed="1000Gf"/>
	<host id="host-4.simgrid.org" speed="1000Gf"/>
	<link id="link-0" latency="10us" bandwidth="10Gbps"/>
	<link id="link-1" latency="10us" bandwidth="10Gbps"/>
	<link id="link-2" latency="10us" bandwidth="10Gbps"/>
	<link id="link-3" latency="10us" bandwidth="10Gbps"/>
	<link id="link-4" latency="10us" bandwidth="10Gbps"/>
	<route src="host-0.simgrid.org" dst="host-1.simgrid.org" symmetrical="NO">
		<link_ctn id="link-0"/>
		<link_ctn id="link-1"/>
	</route>
	<route src="host-1.simgrid.org" dst="host-2.simgrid.org" symmetrical="NO">
		<link_ctn id="link-1"/>
		<link_ctn id="link-2"/>
	</route>
	<route src="host-2.simgrid.org" dst="host-3.simgrid.org" symmetrical="NO">
		<link_ctn id="link-2"/>
		<link_ctn id="link-3"/>
	</route>
	<route src="host-3.simgrid.org" dst="host-4.simgrid.org" symmetrical="NO">
		<link_ctn id="link-3"/>
		<link_ctn id="link-4"/>
	</route>
	<route src="host-4.simgrid.org" dst="host-0.simgrid.org" symmetrical="NO">
		<link_ctn id="link-4"/>
		<link_ctn id="link-0"/>
	</route>
</zone>
</platform>
